{
  "title": "CAA must NOT be able to Update supplementary data of any PRIVATE case that requires explicit access",
  "_guid_": "S-613",
  "productName": "CCD Data Store api",
  "operationName": "Update Supplementary Data",
  "_extends_": "S-613_Update_Supplementary_Data_C2_PRIVATE",
  "specs": [
    "a valid PUBLIC profile"
  ],

  "users": {
    "invokingUser": {
      "_extends_": "BeftaCaseworker2Solicitor1"
    }
  },
  "request": {
    "pathVariables": {
      "caseId": "${[scenarioContext][childContexts][S-613_Case_Data_Create_C2_PRIVATE][testData][actualResponse][body][id]}"
