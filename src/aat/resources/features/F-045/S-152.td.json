{
  "_guid_": "S-152",
  "_extends_": "Base_Expecting_404",
  "title": "must return 404 if case id is invalid",
  "specs": [
    "a detailed profile in CCD",
    "doesn't provide a valid case-reference in input parameters",
    "has the 404 return code"
  ],
  "productName": "CCD Data Store",
  "operationName": "Grant access to case",
  "method": "POST",
  "uri": "/caseworkers/{uid}/jurisdictions/{jid}/case-types/{ctid}/cases/{cid}/users",
  "request": {
    "pathVariables": {
      "uid": "[[DYNAMIC]]",
      "jid": "AUTOTEST1",
      "ctid": "AAT",
      "cid": "157375164884070312"
    },
    "body": {
      "id": "Test User"
    }
  },
  "expectedResponse": {

    "body" : {
      "exception" : "uk.gov.hmcts.ccd.domain.service.getcase.CaseNotFoundException",
      "timestamp" : "[[ANY]]",
      "status" : 404,
      "error" : "Not Found",
      "message" : "[[ANY]]",
      "path" : "[[ANY]]",
      "details" : null,
      "callbackErrors" : null,
      "callbackWarnings" : null
    }
  }
}
