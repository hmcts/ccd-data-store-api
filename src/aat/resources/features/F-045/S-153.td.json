    "pathVariables": {
      "uid": "some id",
      "jid": "AUTOTEST1",
      "ctid": "AAT",
      "cid": "1573751648840703"
    },
    "body": {
      "id": "Test User"
    }
  },
  "expectedResponse": {
    "headers" : {
      "Transfer-Encoding" : "chunked"
    },
    "body" : {
      "timestamp" : "[[ANY]]",
