{
  "_guid_": "S-223",
  "_extends_": "Base_Expecting_404",
  "title": "must return 400 when an invalid case id is provided",
  "specs": [
    "a detailed profile in CCD",
    "contains an invalid case id",
    "has a 404 not found code"
  ],
  "productName": "CCD Data Store",
  "operationName": "Revoke access to case",
  "method": "DELETE",
  "uri": "/caseworkers/{uid}/jurisdictions/{jid}/case-types/{ctid}/cases/{cid}/users/{idToDelete}",
  "request": {
    "pathVariables": {
      "uid": "[[DYNAMIC]]",
      "jid": "AUTOTEST1",
      "ctid": "AAT",
      "cid": "1234567890123456",
      "idToDelete": "[[DYNAMIC]]"
    }
  },
  "expectedResponse": {
    "responseCode": 404,
    "responseMessage": "Not Found",
    "headers": {
      "Content-Length": "[[ANY]]",
      "Content-Encoding" : "gzip",
      "Content-Type" : "application/json;charset=UTF-8"
    },
    "body": {
      "exception" : "uk.gov.hmcts.ccd.domain.service.getcase.CaseNotFoundException",
      "timestamp": "[[ANY]]",
      "status": 404,
      "error": "Not Found",
      "message": "No case found for reference: 1234567890123456",
      "path": "[[ANY]]",
      "details" : null,
      "callbackErrors" : null,
      "callbackWarnings" : null
    }
  }
}
