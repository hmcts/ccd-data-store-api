{
	"title": "should retrieve case links for a specified case reference",
	"_guid_": "F-1018_VerifyMultipleLinkedCaseLinksUnchanged",
  "_extends_": "F-1018_VerifyCaseLinksBase",

  "specs": [
    "to verify that the Case Links in the CASE_LINK table are unchanged"
  ],

	"request": {
		"pathVariables": {
			"cid": "${[scenarioContext][parentContext][childContexts][F-1018_CreateMultipleLinkedCasePreRequisiteCaseworkerBase][testData][actualResponse][body][id]}"
		}
	},

	"expectedResponse": {
		"body": {
      "CaseLinks" : [
      {
        "__ordering__": "UNORDERED",
        "__elementId__": "LinkedCaseReference"
      },
      {
        "CaseReference" : "${[scenarioContext][parentContext][childContexts][F-1018_CreateMultipleLinkedCasePreRequisiteCaseworkerBase][testData][actualResponse][body][id]}",
        "LinkedCaseReference" : "${[scenarioContext][parentContext][childContexts][F-1018_CreateCasePreRequisiteCaseworkerBase][testData][actualResponse][body][id]}",
        "CaseTypeId" : "FT_MasterCaseType"
      },
      {
        "CaseReference" : "${[scenarioContext][parentContext][childContexts][F-1018_CreateMultipleLinkedCasePreRequisiteCaseworkerBase][testData][actualResponse][body][id]}",
        "LinkedCaseReference" : "${[scenarioContext][parentContext][childContexts][F-1018_CreateAnotherCasePreRequisiteCaseworkerBase][testData][actualResponse][body][id]}",
        "CaseTypeId" : "FT_MasterCaseType"
      }]
		}
	}
}
