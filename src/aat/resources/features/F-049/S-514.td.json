{
  "_guid_": "S-514",
  "_extends_": "Base_Expecting_404",
  "title": "must return 404 when no case found for the given Event ID",
  "specs": [
    "an existing case in CCD",
    "a non-existing Event ID in CCD",
    "Start case creation as Case worker",
    "CCD Data Store",
    "return 404"
  ],
  "productName": "CCD Data Store",
  "operationName": "Start case creation as Case worker",
  "method" : "GET",
  "uri": "/caseworkers/{uid}/jurisdictions/{jid}/case-types/{ctid}/cases/{cid}/event-triggers/{etid}/token",
  "request": {
    "pathVariables": {
      "uid": "[[DYNAMIC]]",
      "jid": "AUTOTEST1",
      "ctid": "AAT",
      "cid": "1574079355345891",
      "etid": "CREATE1"
    }
  },
  "expectedResponse": {
    "body" : {
      "exception" : "uk.gov.hmcts.ccd.endpoint.exceptions.ResourceNotFoundException",
      "timestamp" : "[[ANY]]",
      "status" : 404,
      "error" : "Not Found",
      "message" : "Cannot find event CREATE1 for case type AAT",
      "path" : "[[ANY]]",
      "details" : null,
      "callbackErrors" : null,
      "callbackWarnings" : null
    }
  }
}
