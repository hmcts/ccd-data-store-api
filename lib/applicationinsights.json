{
  "connectionString": "${file:/mnt/secrets/ccd/app-insights-connection-string}",
  "role": {
    "name": "ccd-data-store-api"
  },
  "preview": {
    "sampling": {
      "percentage": 50,
      "overrides": [
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "http.url",
              "value": "https?://[^/]+/health.*",
              "matchType": "regexp"
            },
            {
              "key": "http.url",
              "value": "https?://[^/]+/metrics.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 1
        },
        {
          "telemetryType": "dependency",
          "percentage": 25
        },
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "http.url",
              "value": ".*(/documents|/cases/[^/]+/documents).*",
              "matchType": "regexp"
            }
          ],
          "percentage": 15
        }
      ]
    }
  }
}
